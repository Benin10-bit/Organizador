{
  "color_scheme": {
    "name": "organize-ai",
    "type": "dark",
    "theme": {
      "primary_color": "#4A8DFF",
      "secondary_color": "#356DDB",
      "accent_color": "#6CA5FF",

      "success": "#4CCB5A",
      "danger": "#F14A4A",
      "warning": "#FFC857",
      "info": "#33B5E5",

      "bg_dark": "#0A0C10",
      "bg_medium": "#12141A",
      "bg_light": "#1A1C24",

      "text_primary": "#FFFFFF",
      "text_secondary": "#A9ACB6",
      "text_disabled": "#5F636B"
    }
  },

  "CTk": {
    "corner_radius": 8,
    "border_width": 0,
    "fg_color": ["#0A0C10", "#0A0C10"],
    "text_color": ["#ffffff", "#ffffff"]
  },

  "CTkFrame": {
    "corner_radius": 12,
    "border_width": 0,
    "fg_color": ["#12141A", "#12141A"],
    "top_fg_color": ["#0A0C10", "#0A0C10"],
    "border_color": ["#0A0C10", "#0A0C10"]
  },

  "CTkLabel": {
    "corner_radius": 0,
    "fg_color": "transparent",
    "text_color": ["#FFFFFF", "#FFFFFF"],
    "text_color_disabled": ["#5F636B", "#5F636B"]
  },

  "CTkButton": {
    "corner_radius": 8,
    "border_width": 0,
    "fg_color": ["#4A8DFF", "#4A8DFF"],
    "hover_color": ["#356DDB", "#356DDB"],
    "border_color": "transparent",
    "text_color": ["#FFFFFF", "#FFFFFF"],
    "text_color_disabled": ["#A0A0A0", "#A0A0A0"]
  },

  "CTkEntry": {
    "corner_radius": 8,
    "border_width": 2,
    "border_color": ["#1D2230", "#1D2230"],
    "fg_color": ["#1A1C24", "#1A1C24"],
    "text_color": ["#FFFFFF", "#FFFFFF"],
    "placeholder_text_color": ["#5F636B", "#5F636B"]
  },

  "CTkTextbox": {
    "corner_radius": 10,
    "border_width": 1,
    "fg_color": ["#0C0E12", "#0C0E12"],
    "border_color": ["#1A2437", "#1A2437"],
    "text_color": ["#E5E5E5", "#D0D0D0"],

    "scrollbar_fg_color": ["#121820", "#121820"],
    "scrollbar_button_color": ["#1E2A3D", "#1E2A3D"],
    "scrollbar_button_hover_color": ["#2A3B57", "#2A3B57"],

    "font": {
      "family": "Segoe UI",
      "size": 13,
      "weight": "normal"
    }
  },

  "CTkScrollableFrame": {
    "corner_radius": 12,
    "border_width": 0,
    "border_color": "transparent",
    "fg_color": ["#12141A", "#12141A"],
    "top_fg_color": ["#12141A", "#12141A"],
    "label_fg_color": "transparent"
  },

  "CTkOptionMenu": {
    "corner_radius": 8,
    "fg_color": ["#1A1C24", "#1A1C24"],
    "button_color": ["#4A8DFF", "#4A8DFF"],
    "button_hover_color": ["#356DDB", "#356DDB"],
    "text_color": ["#FFFFFF", "#FFFFFF"]
  },

  "CTkComboBox": {
    "corner_radius": 8,
    "fg_color": ["#1A1C24", "#1A1C24"],
    "border_width": 2,
    "border_color": ["#1D2230", "#1D2230"],
    "button_color": ["#4A8DFF", "#4A8DFF"],
    "button_hover_color": ["#356DDB", "#356DDB"],
    "text_color": ["#FFFFFF", "#FFFFFF"],
    "dropdown_fg_color": ["#12141A", "#12141A"],
    "dropdown_text_color": ["#FFFFFF", "#FFFFFF"]
  },

  "CTkCheckBox": {
    "corner_radius": 6,
    "fg_color": ["#1A1C24", "#1A1C24"],
    "border_color": ["#4A8DFF", "#4A8DFF"],
    "checkmark_color": ["#4A8DFF", "#4A8DFF"],
    "text_color": ["#FFFFFF", "#FFFFFF"]
  },

  "CTkProgressBar": {
    "corner_radius": 8,
    "fg_color": ["#1A1C24", "#1A1C24"],
    "progress_color": ["#4A8DFF", "#4A8DFF"],
    "border_color": "transparent"
  },

  "CTkFont": {
    "family": "Segoe UI",
    "size": 14,
    "weight": "normal"
  },

  "CTkScrollbar": {
    "corner_radius": 1000,
    "border_spacing": 4,
    "fg_color": ["#121820", "#121820"],
    "button_color": ["#1E2A3D", "#1E2A3D"],
    "button_hover_color": ["#2A3B57", "#2A3B57"]
  }
}
